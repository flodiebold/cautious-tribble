    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1",
    "webpack": "^4.23.1",
    "webpack": "^4.26.1",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.3"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
