    "prettier": "^1.15.3",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1",
    "webpack": "^4.28.3",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
